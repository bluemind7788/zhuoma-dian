    "cross-env": "^3.1.3",
    "element-ui": "^1.2.7",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "fastclick": "^1.0.6",
    "firebase": "^3.6.2",
    "lru-cache": "^4.0.2",
    "multiparty": "^4.1.3",
    "mysql": "^2.13.0",
    "postcss-px2rem": "^0.3.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "socket.io": "^1.7.3",
    "vue": "^2.2.6",
    "vue-echarts": "^2.3.7",
    "vue-qriously": "^1.0.0",
    "vue-router": "^2.3.0",
    "vue-server-renderer": "^2.2.4",
    "vue-style-loader": "^2.0.4",
